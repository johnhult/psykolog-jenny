	"scripts": {
		"build": "onerror 'webpack --config webpack.prod.js --mode production --display-error-details' -t 'The build has failed' -m 'Check the console to find out what went wrong.' -s 'Submarine'",
		"build:netlify": "webpack --config webpack.prod.js --mode production --display-error-details && npm run copy:meta && npm run test:size-limit",
		"build:storybook": "build-storybook",
		"commands": "npx ntl",
		"copy:assets": "cp -R src/assets dist/ && cp src/* dist/",
		"copy:meta": "cp src/robots.txt dist/ && cp src/favicon.ico dist/ && cp src/sitemap.xml dist/ && cp src/crossdomain.xml dist/ && cp src/_headers dist/ && cp src/_redirects dist/ && cp src/manifest.webmanifest dist/",
		"dev": "webpack-dev-server --config webpack.dev.js --open --mode development",
		"figmagic": "./node_modules/figmagic/index.mjs",
		"optimize:graphics": "svgo -f ./dist/assets/**/ --multipass --disable=removeViewBox --enable=removeDimensions",
		"plop": "npx plop",
		"postbuild": "yarn copy:meta && yarn test:size-limit",
		"precommit": "echo Running pre-commit testing and linting... && yarn build && pretty-quick --staged && npm --no-git-tag-version version patch && git add package.json",
		"setup": "yarn && echo 'âœ”' Used Yarn to install all dependencies && npx storybook init -f && yarn setup:welcome",
		"setup:welcome": "echo 'âœ”' Set up React Storybook && echo 'ğŸ‰' New commands are available! 'ğŸ‰' && echo 'â€”' yarn dev: Start development server && echo 'â€”' yarn build: Output a static build && echo 'â€”' yarn plop function/class/model: Create function/class/model with test for function/class && echo 'â€”' yarn plop component: Create React component, with Styled Components file and Storybook story && echo 'â€”' yarn plop componentclass: Create React classed component, with Styled Components file and Storybook story && echo 'â€”' yarn plop view/composition/context/provider: Create basic React items of specified type && echo 'â€”' yarn storybook: Start React Storybook && echo 'â€”' yarn build:storybook: Build React Storybook && echo 'â€”' yarn commands: See a list of all available commands",
		"start": "test && yarn run dev",
		"storybook": "start-storybook -p 6006",
		"test": "yarn test:all",
		"test:accessibility": "npx axe $npm_package_config_siteUrl --save axe.report.json && npx pa11y --reporter csv $npm_package_config_siteUrl > pa11y.report.csv",
		"test:performance": "npx lighthouse $npm_package_config_siteUrl --view",
		"test:all": "yarn test:size-limit && yarn test:visual && yarn test:jest && yarn test:e2e && yarn test:visual",
		"test:jest": "jest",
		"test:e2e": "jest __tests__/e2e",
		"test:e2e:debug": "NODE_ENV=debug yarn test:e2e",
		"test:visual": "jest __tests__/visual",
		"test:size-limit": "size-limit",
		"test:hint": "npx hint $npm_package_config_siteUrl"
	},
